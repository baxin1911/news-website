<div class="<%= classContainer %>">
    <% buttons.forEach(button => { %>
        <a class="<%= withStyle2 ? button.style2 : button.style1 %>" href="<%= button.slug %>">
            <% if (withIcon) { %>
                <i class="<%= button.icon %>"></i>
            <% } %>

            <% if (withName) { %>
                <%= button.name %>
            <% } %>
        </a>
    <% }); %>
</div>